#define DLog(_format_, ...)             NSLog(_format_, ## __VA_ARGS__)
#else
#define DLog(_format_, ...)
#endif
#endif


#define NULL_TO_NIL(obj)({ __typeof__ (obj) __obj = (obj); __obj == [NSNull null] ? nil : obj;})
#define NIL_TO_STRING(str) str == nil ? @"" : str;

